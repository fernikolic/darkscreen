{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": [
    {
      "site": "clawdentials",
      "public": "web/dist",
      "ignore": [
        "firebase.json",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "source": "/.well-known/**",
          "headers": [
            { "key": "Content-Type", "value": "application/json" },
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "/llms.txt",
          "headers": [
            { "key": "Content-Type", "value": "text/plain; charset=utf-8" }
          ]
        },
        {
          "source": "/robots.txt",
          "headers": [
            { "key": "Content-Type", "value": "text/plain" }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "clawdentials-prod",
      "public": "web/dist",
      "ignore": [
        "firebase.json",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "source": "/.well-known/**",
          "headers": [
            { "key": "Content-Type", "value": "application/json" },
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "/llms.txt",
          "headers": [
            { "key": "Content-Type", "value": "text/plain; charset=utf-8" }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
